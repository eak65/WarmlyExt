{
  "manifest_version": 2,
  "name": "Warmly",
  "version": "0.4.7",
  "description": "Automated web research assistant.",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["scripts/content.js", "scripts/result.js"],
      "css": ["styles/content.css", "styles/selectize.default.css"]
    }
  ],
  "web_accessible_resources": [
    "scripts/jquery-1.10.2.min.js",
    "scripts/gmail.js",
    "scripts/main.js",
    "scripts/result.html",
    "scripts/result.js",
    "scripts/selectize.js"
  ],
  "externally_connectable": {
    "matches": [
      "*://mail.google.com/*",
      "chrome-extension://papmjbnpmffiahcnakjfjoobkefaemii/*"
    ]
  },
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "notifications",
    "storage",
    "*://*/*"
  ],
  "content_security_policy":
  "script-src 'self' chrome-extension://papmjbnpmffiahcnakjfjoobkefaemii/*;  object-src 'self'",
  "key": ""
}
