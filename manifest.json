{
  "manifest_version": 2,
  "name": "Warmly",
  "version": "0.4.8",
  "description": "Automated web research assistant.",
  "content_scripts": [
    {
      "matches": ["*://mail.google.com/*"],
      "js": ["js/content.js", "js/result.js"],
      "css": ["css/content.css", "css/selectize.default.css"]
    }
  ],
  "web_accessible_resources": [
    "js/jquery-1.10.2.min.js",
    "js/gmail.js",
    "js/main.js",
    "js/result.html",
    "js/result.js",
    "js/selectize.js"
  ],
  "externally_connectable": {
    "matches": [
      "*://mail.google.com/*",
      "chrome-extension://papmjbnpmffiahcnakjfjoobkefaemii/*"
    ]
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "notifications",
    "storage",
    "*://*/*"
  ],
  "content_security_policy":
  "script-src 'self' chrome-extension://papmjbnpmffiahcnakjfjoobkefaemii/*;  object-src 'self'",
  "key": ""
}
